{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/subComponents/*": [
        "subComponents/*"
      ],
      "@/images/*": [
        "public/images/*"
      ],
      "@/svgs/*": [
        "public/svgs/*"
      ],
      "@/videos/*": [
        "public/videos/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/_app.tsx", "pages/api/apollo-client.js", "pages/queries/queries.js", "pages/api/[post].js"  ],
  "exclude": [
    "node_modules"
  ]
}
